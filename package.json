{
  "name": "prisma-next-graphql",
  "description": "Minimal GraphQL boilerplate with Prisma and Next.js",
  "version": "0.0.0-development",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "tsc": "tsc -p tsconfig.dev.json",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
    "commit": "git-cz",
    "db-start": "docker-compose up",
    "db-view": "env-cmd -f envs/dev.env npx prisma studio",
    "db-migration-generate": "env-cmd -f envs/dev.env npx prisma migrate dev",
    "db-migration-run": "env-cmd -f envs/${ENV}.env npx prisma migrate deploy",
    "db-migration-run-dev": "ENV=dev npm run db-migration-run",
    "db-migration-run-stage": "ENV=stage npm run db-migration-run",
    "db-migration-run-prod": "ENV=prod npm run db-migration-run",
    "db-seed-run": "env-cmd -f envs/${ENV}.env ts-node --project tsconfigs/tsconfig.prisma-seed.json prisma/seed/index.ts",
    "db-seed-run-dev": "ENV=dev npm run db-seed-run",
    "db-reset": "env-cmd -f envs/${ENV}.env npx prisma migrate reset -f",
    "db-reset-dev": "ENV=dev npm run db-reset"
  },
  "dependencies": {
    "next": "12.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@prisma/client": "^4.0.0",
    "@types/eslint": "^8.4.5",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "commitizen": "^4.2.4",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-array-reduce": "^1.0.60",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.0.0",
    "ts-node": "^10.8.2",
    "tsc-files": "^1.1.3",
    "typescript": "^4.7.4"
  }
}
